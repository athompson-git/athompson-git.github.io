    "LDMX": {
        "Class": "Beam Target",
        "Source(s)": "",
        "Beam Particle": "electron",
        "Beam Energy (GeV)": "o(10) GeV",
        "POT": "4.00E+14",
        "Baseline (m)": "",
        "Detector Tech": "",
